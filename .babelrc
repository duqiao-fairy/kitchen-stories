{
  "presets": [
    ["env", { "modules": false }],
    "stage-2"
  ],
  "plugins": [
    "transform-runtime", 
    "transform-vue-jsx",
    [
      "component", 
      [
        {
          "libraryName": "mint-ui",
          "style": false
        }
      ]
    ]
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [ "istanbul" ]
    }
  }
}
